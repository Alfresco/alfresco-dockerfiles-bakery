{
  "$schema": "../../../node_modules/@alfresco/adf-core/app.config.schema.json",
  "providers": "<!--# echo var=\"appConfigProvider\" default=\"ALL\" -->",
  "ecmHost": "<!--# echo var=\"appConfigEcmHost\" default=\"{protocol}//{hostname}{:port}\" -->",
  "bpmHost": "<!--# echo var=\"appConfigBpmHost\" default=\"{protocol}//{hostname}{:port}\" -->",
  "authType": "<!--# echo var=\"appConfigAuthType\" default=\"OAUTH\" -->",
  "identityHost": "<!--# echo var=\"appConfigIdentityHost\" default=\"{protocol}//{hostname}{:port}/auth/admin/realms/alfresco\" -->",
  "oauth2": {
    "host": "<!--# echo var=\"appConfigOauth2Host\" default=\"{protocol}//{hostname}{:port}/auth/realms/alfresco\" -->",
    "clientId": "<!--# echo var=\"appConfigOauth2ClientId\" default=\"alfresco\" -->",
    "scope": "<!--# echo var=\"appConfigOauth2Scope\" default=\"openid profile email\" -->",
    "implicitFlow": "<!--# echo var=\"appConfigOauth2ImplicitFlow\" default=\"true\" -->",
    "codeFlow": "<!--# echo var=\"appConfigOauth2CodeFlow\" default=\"false\" -->",
    "silentLogin": "<!--# echo var=\"appConfigOauth2SilentLogin\" default=\"true\" -->",
    "publicUrls": [],
    "redirectSilentIframeUri": "<!--# echo var=\"appConfigOauth2RedirectSilentIframeUri\" default=\"{protocol}//{hostname}{:port}/assets/silent-refresh.html\" -->",
    "redirectUri": "<!--# echo var=\"appConfigOauth2RedirectUri\" default=\"\" -->",
    "audience": "<!--# echo var=\"appConfigOauth2Audience\" default=\"\" -->",
    "redirectUriLogout": "<!--# echo var=\"appConfigOauth2RedirectUriLogout\" default=\"/\" -->",
    "skipIssuerCheck": true,
    "strictDiscoveryDocumentValidation": false
  },
  "application": {
    "name": "Alfresco Admin App",
    "copyright": "Â© 2005-2024 Hyland Software, Inc. and its affiliates. All rights reserved."
  },
  "plugins": {
    "tagsEnabled": "<!--# echo var=\"appConfigPluginTags\" default=\"true\" -->",
    "categoriesEnabled": "<!--# echo var=\"appConfigPluginCategories\" default=\"true\" -->"
  },
  "logLevel": "<!--# echo var=\"appConfigLogLevel\" default=\"trace\" -->",
  "locale": "en",
  "pagination": {
    "size": 50,
    "supportedPageSizes": [25, 50, 100]
  },
  "export": {
    "helm": true
  },
  "alfresco-dependencies": [
    "rb",
    "query"
  ],
  "content-identity": {
    "presets": {
      "default": [
        {
          "key": "id",
          "type": "text",
          "title": "CONTENT_IDENTITY_USERS.USER_LIST.PROPERTIES.ID",
          "sortable": true,
          "draggable": true
        },
        {
          "key": "firstName",
          "type": "text",
          "title": "CONTENT_IDENTITY_USERS.USER_LIST.PROPERTIES.FIRST_NAME",
          "sortable": true,
          "draggable": true
        },
        {
          "key": "lastName",
          "type": "text",
          "title": "CONTENT_IDENTITY_USERS.USER_LIST.PROPERTIES.LAST_NAME",
          "sortable": true,
          "draggable": true
        },
        {
          "key": "email",
          "type": "text",
          "title": "CONTENT_IDENTITY_USERS.USER_LIST.PROPERTIES.EMAIL",
          "sortable": false,
          "draggable": true
        },
        {
          "key": "status",
          "type": "text",
          "title": "CONTENT_IDENTITY_USERS.USER_LIST.PROPERTIES.STATUS",
          "sortable": false,
          "draggable": true
        }
      ]
    },
    "group-list-presets": {
      "default": [
        {
          "key": "displayName",
          "type": "text",
          "title": "CONTENT_IDENTITY_GROUPS.GROUP_LIST.PROPERTIES.DISPLAY_NAME",
          "sortable": false
        }
      ]
    }
  },
  "security-controls": {
    "presets": {
      "default": []
    }
  }
}
